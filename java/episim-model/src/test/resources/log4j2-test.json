{
	"configuration":{
		"loggers":{
			"root": 
				{ "level": "trace", "AppenderRef":{ "ref": "STDOUT" } },
			"logger": [
            	{ "name": "org.eclipse.jetty", "level": "warn", "AppenderRef":{ "ref": "STDOUT" }}
    	    ]
		},
		"appenders":{
			"Console":{
				"name":"STDOUT",
				"PatternLayout":{
					"pattern":"%d{HH:mm:ss.SSS} %-5p [%t] %m [at %C.%M(%F:%L)]%n"
				}
			}
		}
	}
}